{
  "name": "projeto",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev:backend": "nest start --watch",
    "dev:frontend": "vite",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:backend": "nest build",
    "build:frontend": "vite build",
    "build": "npm run build:backend && npm run build:frontend && npm run copy-uploads",
    "preview": "vite preview",
    "format": "prettier --write src/",
    "start:backend": "node dist/backend/main.js",
    "start:frontend": "vite preview --root dist/frontend",
    "start": "npm run dev",
    "copy-uploads": "node copy-uploads.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.19",
    "@nestjs/core": "^11.0.19",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.0.19",
    "@nestjs/swagger": "^11.1.3",
    "@nestjs/typeorm": "^11.0.0",
    "@types/express": "^5.0.1",
    "axios": "^1.8.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "express": "^4.21.2",
    "mongodb": "^6.0.0",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.2",
    "swagger": "^0.0.1",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.22",
    "vue": "^3.5.13",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",

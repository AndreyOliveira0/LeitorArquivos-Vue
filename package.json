    "@vitejs/plugin-vue": "^5.2.3",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "ncp": "^2.0.0",
    "nodemon": "^3.0.1",
    "prettier": "3.5.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
